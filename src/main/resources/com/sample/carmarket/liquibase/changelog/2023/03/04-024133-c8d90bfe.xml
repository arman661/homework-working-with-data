<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.11.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="homework-carmarket">
        <addUniqueConstraint columnNames="REGISTRATION_NUMBER" constraintName="IDX_CAR_REGISTRATION_NUMBER"
                             tableName="CAR"/>
    </changeSet>
    <changeSet id="2" author="homework-carmarket">
        <dropIndex indexName="IDX_CAR_MODEL" tableName="CAR"/>
    </changeSet>
    <changeSet id="3" author="homework-carmarket">
        <createIndex indexName="IDX_CAR_MODEL" tableName="CAR" unique="false">
            <column name="MODEL_ID"/>
        </createIndex>
    </changeSet>
</databaseChangeLog>